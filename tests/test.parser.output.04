(define insert (lambda (v bst) (cond ((null-binary-tree? bst) (list v quote () quote ())) ((< v (entry bst)) (list (entry bst) (insert v (left bst)) (right bst))) ((= v (entry bst)) (list (entry bst) (left bst) (right bst))) ((> v (entry bst)) (list (entry bst) (left bst) (insert v (right bst)))))))
